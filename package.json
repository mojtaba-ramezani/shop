{
  "name": "shop",
  "version": "0.0.0",
  "author": "Mojtaba Ramezani <ramezanimojtaba72@gmail.com>",
  "description": "shop website built using Angular4, NodeJs, Express and Mongoose, whole stack in TypeScript.",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "start": "node build/server/app.js",
    "predev": "tsc -p server",
    "server": "concurrently \"mongod\" \"ng serve -pc proxy.conf.json --open\" \"tsc -w -p server\" \"nodemon build/server/app.js\"",
    "client": "concurrently \"mongod\" \"ng build -aot -prod && tsc -p server && node build/server/app.js\"",
    "test": "ng test",
    "testbe": "tsc -p server && node_modules/.bin/mocha build/server/test",
    "lint": "ng lint",
    "lintbe": "tslint server/**/**.ts{,x}",
    "e2e": "ng e2e"
  },
  "private": true,
